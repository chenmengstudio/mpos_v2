<?xml version="1.0" encoding="UTF-8"?>
<datasyncconfig>
	<!-- 执行指令任务的方法  -->
	<commandtype>
		<type>
		  <typevalue>1</typevalue>
			<call>command1</call>
		</type>
		<type>
		  <typevalue>2</typevalue>
			<call>command2</call>
		</type>
		<type>
		  <typevalue>3</typevalue>
			<call>command3</call>
		</type>
		<type>
		  <typevalue>4</typevalue>
			<call>command4</call>
		</type>
	</commandtype>
	<!-- 执行数据更新任务的方法  -->
	<updatetype>
		<type>
		  <typevalue>1</typevalue>
			<call>update1</call>
		</type>
		<type>
		  <typevalue>2</typevalue>
			<call>update2</call>
		</type>
		<type>
		  <typevalue>3</typevalue>
			<call>update3</call>
		</type>
		<type>
		  <typevalue>4</typevalue>
			<call>update4</call>
		</type>
	</updatetype>
	<!-- temp表拉数据到主表时，有些表不用全部清空主表数据
	tablename是表名,field是对应的字段
	单个字段例:<field>code</field>
	多个字段例:<field>code1 || code2</field>
	  -->
	<notdeletetables>
		<table>
			<tablename>TBPRODUCTSORT</tablename>
			<field>code</field>
		</table>
		<table>
			<tablename>TBSHOPGDINPUT</tablename>
			<field>code</field>
		</table>
		<table>
			<tablename>TBPOSCKDIR</tablename>
			<field>OrderID</field>
		</table>
	</notdeletetables>
</datasyncconfig>